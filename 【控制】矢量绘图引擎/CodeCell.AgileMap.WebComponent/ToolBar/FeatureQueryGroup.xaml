<UserControl x:Class="CodeCell.AgileMap.WebComponent.FeatureQueryGroup"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
     xmlns:toolbar="clr-namespace:Windows.Toolbar.Controls;assembly=Windows.Toolbar"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    <toolbar:ToolbarGroup Footer="查询/定位">
        <StackPanel Orientation="Horizontal">
            <TextBox KeyDown="txtKeyWords_KeyDown" Height="24" Name="txtKeyWords" Width="160" Background="#FFECEEF0"/>
            <Button Margin="2" Click="btnGo_Click" Width="32" Name="btnGo" Content="GO"/>
        </StackPanel>
    </toolbar:ToolbarGroup>
</UserControl>
