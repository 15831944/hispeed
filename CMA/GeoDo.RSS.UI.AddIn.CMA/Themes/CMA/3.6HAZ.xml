              <Argument name="NDSIMax" description="积雪指数高端值" endpointtype="max" defaultvalue="0.2" />
            </ArgumentPair>
            <ArgumentPair description="中红外远红外差异阈值" datatype="float" minvalue="0" maxvalue="150">
