      <Algorithms>
        <Algorithm name="雾霾产品生产" identify="HAZEAlgorithm" satellite="FY3A,FY3B,FY3C" sensor="VIRR" customidentify="" >
          <Arguments>
            <Argument name="TOUTxt" description="AI指数数据"
